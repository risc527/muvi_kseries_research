<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="18.0dip" />
            <gradient android:startColor="#ff292929" android:endColor="#ff292929" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff292929" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="18.0dip" />
                <gradient android:startColor="#ffffff00" android:endColor="#ffffff00" android:angle="270.0" android:centerY="0.45" android:centerColor="#ffffff00" />
            </shape>
        </clip>
    </item>
</layer-list>